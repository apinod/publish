                headerTag: "h3",
                bodyTag: "section",
                transitionEffect: "slideLeft",
                //autoFocus: true,
                enableFinishButton: false,
                onStepChanging: function (event, currentIndex, newIndex) {                    
                    if (newIndex > currentIndex)
                    {
                        if($("#register-form").valid() == false)
                            return false;
                    }
                    visited[currentIndex] = true;

                    return true;
                },

                onStepChanged: function (event, currentIndex, priorIndex) {
                    if (visited[currentIndex] == true || currentIndex == 0)
                        if ($("#register-form").valid() == false)
                            $("#register-form li.current").addClass("error");
                }
            });
            
            $("#register-form #mode-list li").click(function (target) {

                var opts = $(this).siblings();
                
                $(opts).each(function (index, element) {
                    $(element).removeClass('btn-primary');
                    $(element).addClass('btn-default');
                });

                var mode = $('#register-form #user_mode');
                //var debit = $('#register-form #debit_info input, #register-form #debit_info select');
                //var credit = $('#register-form #credit_info input, #register-form #credit_info select');

                switch (this.getAttribute('id')) {
                    case "biller_opt":
                        //$(debit).removeAttr('disabled');
                        //$(credit).attr('disabled', 'disabled');
                        //$(credit).val('');
                        $(mode).val(1);
                        break;
                    case "payor_opt":
                        //$(credit).removeAttr('disabled');
                        //$(debit).attr('disabled', 'disabled');
                        //$(debit).val('');                        
                        $(mode).val(2);
                        break;
                    case "both_opt":
                        //$(debit).removeAttr('disabled');
                        //$(credit).removeAttr('disabled');
                        $(mode).val(3);
                        break;
                }

                $(this).removeClass('btn-default');
                $(this).addClass('btn-primary');
            });

            $('button#b').click(formSubmitHandler);
            //    function () {
            //    $('form#register-form').valid();
            //});

            $('#btn_replicate_right').click(function () {

                var credit = $('div#credit_info input');
                $('div#debit_info input').each(function (index, element) {                    
                    $(credit[index]).val($(element).val());
                });

                $('div#credit_info select').val($('div#debit_info select').val());
                $("#register-form").valid();
            });

            $('#btn_replicate_left').click(function () {

                var debit = $('div#debit_info input');
                $('div#credit_info input').each(function (index, element) {
                    $(debit[index]).val($(element).val());
                });

                $('div#debit_info select').val($('div#credit_info select').val());
                $("#register-form").valid();
            });
        });

        function closePopup() {
            $.magnificPopup.close();
        }

        var generateCaptcha = function myfunction() {

            $("a#btn-submit").attr('disabled', 'disabled');
            $('#image').addClass('mfp-hide');
            $('#captcha-loader').removeClass('mfp-hide');

            $.ajax({
                //url: "http://localhost:53859/Service1.asmx/GetCaptchaImage",
                //url: "/PayAnyBiz-V1/WebService1/WebService1/Service1.asmx/GetCaptchaImage",
                //url: "/CaptchaServiceDeploy/Service1.asmx/GetCaptchaImage",
                //url: "/captcha/Service1.asmx/GetCaptchaImage",
                url: "/Account/GetCaptchaImage",
                //data: { imageText: "John5" },
                crossDomain: true,
                success: function (data, textStatus, jqXHR) {
                    console.log("SUCCESS: " + data.text.toLowerCase());
                    $('#image').html('<img src="data:image/png;base64,' + data.image + '"/>');
                    $('#Bgv4G').val(data.text.toLowerCase());

                    $('#image').removeClass('mfp-hide');
                    $('#captcha-loader').addClass('mfp-hide');
                    $("a#btn-submit").removeAttr('disabled');
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    console.log("ERROR: " + textStatus);
                },
            });
        }
    </script>

